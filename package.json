    "@rollup/plugin-typescript": "^2.0.1",
    "@types/benchmark": "^1.0.31",
    "@types/jasmine": "^3.5.0",
    "@types/node": "^12.12.20",
    "@typescript-eslint/eslint-plugin": "^2.12.0",
    "@typescript-eslint/parser": "^2.12.0",
    "babel-plugin-istanbul": "^5.2.0",
    "benchmark": "^2.1.4",
    "eslint": "^6.7.2",
